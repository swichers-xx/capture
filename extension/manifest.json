{
    "manifest_version": 3,
    "name": "Live Data Sender and History Importer",
    "version": "1.0",
    "permissions": [
      "tabs",
      "history",
      "<all_urls>",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  